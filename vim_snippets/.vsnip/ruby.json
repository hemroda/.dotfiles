{
  "frozen": {
    "prefix": ["frozen_string_literal"],
    "body": [
      "# frozen_string_literal: true",
      "$1"
    ]
  },
  "describe": {
    "prefix": ["describe '' do end"],
    "body": [
      "describe '$1' do",
      "end"
    ]
  },
  "it": {
    "prefix": ["it '' do end"],
    "body": [
      "it '$1' do",
      "end"
    ]
  },
  "Class": {
    "prefix": ["class"],
    "body": [
      "/**",
      " * @author ${VIM:\\$USER}",
      " */",
      "class $1 ${2:extends ${3:Parent} }{",
      "\tconstructor() {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Class definition template."
  }
}
